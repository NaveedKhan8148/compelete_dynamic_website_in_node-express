<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css"
    integrity="sha512-1ycn6IcaQQ40/MKBW2W4Rhis/DbILU74C1vSrLJxCq57o941Ym01SwNsOMqvEBFlcgUa6xLiPY/NS5R+E6ztJQ=="
    crossorigin="anonymous" referrerpolicy="no-referrer" />
  <link rel="stylesheet" href="/static/css/style.css">


  <title>Dynamic website </title>
</head>

<body>
  {{!-- this is the navbar section --}}
  {{>navbar data=details}}
  {{!-- this is the slider --}}
  {{>slider slides=slides}}

  {{!-- this is start learning section --}}

  <div class="section background1">
    <div class="container ">
      <div class="row text-center text-secondary">
        <div class="col md 12 my-5">
          {{#each newTechnology}}
          <h1>{{this.title}}</h1>
          <p>{{this.description}}</p>
          <button type="button" class="btn btn-outline-primary btn-lg">{{this.buttonText}}</button>
          {{/each}}
        </div>
      </div>
    </div>
  </div>

  {{!-- this the services section --}}

  <section id="services_section py-4">
    <div class="container">
      <h1 class="text-center text-secoundry pt-4">Services that we provided</h1>
      <div class="row">
        {{#each services}}
        <div class="col-md-4">
          <div class="card mt-4">
            <div class="card-body">
              <div class="container text-center">
                <i class="{{this.icon}} service_icon text-secondary"></i>
              </div>
              <h5 class="card-title">{{this.title}}</h5>
              <p> {{this.description}}</p>
              <a href="{{this.link}}" class="card-link">{{this.linkText}}</a>

            </div>
          </div>
        </div>
        {{/each}}

      </div>
    </div>
  </section>
  {{!-- servicess section end here --}}

  {{!-- banner section start here --}}

  <section>
    <div class="container my-4 text-center ">
      {{#each section1}}
      <div class="row">
        <div class="col-md-6 py-5 mt-5 text-secondary">
          <h3>{{this.title}}</h3>
          <p>{{this.discription}}
          </p>

          <button type="submit" class="btn btn-primary">{{this.buttonText}}</button>

        </div>
        <div class="col-md-6 mt-2">
          <img src="{{this.imageUrl}}" class="img-fluid" style="height: 350px;" alt="">
        </div>
      </div>
      {{/each}}
    </div>
  </section>
  {{!-- banner1 section end here --}}
  <section>
    <div class="container my-4 text-center ">
      <div class="row">
        {{#each section2}}
        <div class="col-md-6 mt-2 ">
          <img src="{{this.imageUrl}}" class="img-fluid" style="height: 350px;" alt="">
        </div>
        <div class="col-md-6 py-5 mt-5 text-secondary">
          <h3>{{this.title}}</h3>
          <p>{{this.discription}}
          </p>

          <button type="submit" class="btn btn-primary">{{this.buttonText}}</button>

        </div>

      </div>
      {{/each}}
    </div>
  </section>
  {{!-- banner2 section start from here --}}


  {{!-- banner2 section start from here --}}



  {{!-- aboutus section --}}

  <section class="py-5 background1">
    <div class="container">
      <div class="row">
        <h3 class="text-center text-secondary">Contact Us</h3>
        <div class="col-md-12">
          <form action="/process-contact-form" method="post">
            <div class="mb-3">
              <label for="exampleInputEmail1" class="form-label">Your Email address</label>
              <input type="email" name="email" class="form-control" id="exampleInputEmail1"
                aria-describedby="emailHelp">
              <div id="emailHelp" class="form-text text-white">We'll never share your email with anyone else.</div>
            </div>
            <div class="mb-3">
              <label for="exampleInputPassword1" class="form-label">Your Contact Number</label>
              <input type="number" name="phone" class="form-control" id="exampleInputPassword1">
            </div>
            <div class="form-floating">
              <textarea style="height: 400px;" name="query" class="form-control" rows="10 !important"
                placeholder="Leave a comment here" id="floatingTextarea"></textarea>

              <label for="floatingTextarea">Comments</label>
            </div>
            <div class="container text-center ">
              <button type="submit" class="btn btn-dark my-3 btn-lg">Contact Us</button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </section>
  {{!-- contact us section are ended here --}}

  {{!-- about us section is start from here --}}

  <section>
    <div class="container">
      <div class="row text-center my-5 text-secondary">
        {{#each aboutus}}
        <div class="col-md-12">
          <h3>{{this.title}}</h3>
          <p>{{this.discription}}
          </p>
        </div>
        <div class="col-md-12">
          <button type="button" class="btn btn-outline-primary btn-lg">{{this.buttonText}}</button>
        </div>
      </div>
    </div>
    {{/each}}
  </section>

  {{!-- about us section is start from here --}}

  <footer>
    <div class="container-fluid background1">
      <div class="row text-center py-5">
        <div class="col-md-12">
          &copy; Copyrights My Website
        </div>
      </div>
    </div>
  </footer>
  <!-- Option 1: Bootstrap Bundle with Popper -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM"
    crossorigin="anonymous"></script>

</body>

</html>