<nav class="navbar py-4 border-bottom navbar-expand-lg navbar-light bg-light">
  <div class="container">

    <img src="{{data.brandIconUrl}}" style="border-radius: 50%;" width="40" height="40" alt>
    {{#each data.links}}
    {{#if @first}} {{!-- Check if it's the first iteration --}}
    <a class="navbar-brand" href="{{this.url}}">{{../data.brandName}}</a>
    {{/if}}
    {{/each}}
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent"
      aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav ms-auto mb-2 mb-lg-0">
        {{#each data.links}}
        <li class="nav-item">
          <a class="nav-link active" aria-current="page" href="{{this.url}}">{{this.label}}</a>
        </li>
        {{/each}}
      </ul>

    </div>
  </div>
</nav>
